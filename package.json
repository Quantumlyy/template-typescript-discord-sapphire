{
	"name": "--name--",
	"description": "--description--",
	"version": "1.0.0",
	"main": "dist/index.js",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js",
		"#utils/*": "./dist/lib/utils/*.js"
	},
	"author": {
		"name": "QuantumlyTangled",
		"email": "yo@quantumlytangled.com",
		"url": "https://quantumlytangled.com"
	},
	"license": "MIT",
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"update": "yarn upgrade-interactive --latest",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"sversion": "yarn standard-version"
	},
	"dependencies": {
		"@sapphire/framework": "^1.0.0-alpha.4",
		"@sapphire/pieces": "^1.2.0",
		"@sapphire/utilities": "^1.4.4",
		"discord.js": "^12.5.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.2.1",
		"@commitlint/config-conventional": "^13.2.0",
		"@quantumly/eslint-config": "^1.1.0",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.4",
		"@types/ws": "^8.2.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.1.0",
		"husky": "^4.3.8",
		"jest": "^27.3.1",
		"jest-circus": "^27.3.1",
		"lint-staged": "^11.2.4",
		"standard-version": "^9.1.0",
		"ts-jest": "^27.0.7",
		"typescript": "^4.1.3"
	},
	"engines": {
		"node": ">=15",
		"npm": ">=6"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "yarn lint-staged"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix --ext ts"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
